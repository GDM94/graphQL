scalar Date

type Anagrafica{
    idana: ID!
    nome: String!
    cognome: String!
    date_create: Date!
    date_agg: Date!
}

extend type Query {
    anagraficaByid(id: ID!): Anagrafica
    findAllanagrafica: [Anagrafica]!
}

extend type Mutation {
    newAnagrafica(idana: ID!, nome: String!, cognome: String!):Anagrafica!
}